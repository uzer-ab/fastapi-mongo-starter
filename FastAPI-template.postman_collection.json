				{
					"name": "login user",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Handling the response based on the status code",
									"const response = pm.response.json();",
									"if (pm.response.code === 200) {",
									"    pm.environment.set(\"token\", response.data.token.access_token);",
									"    pm.environment.set(\"user_id\", response.data.user.id);",
									"} else {",
									"    console.error(\"Error: \", pm.response.text());",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "accept",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"dev@example.com\",\n  \"password\": \"12345\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{base_url}}/api/v1/auth/login",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
